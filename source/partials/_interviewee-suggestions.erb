    <div class="random-profiles-container">
      <h4>INTERESTING INTERVIEWS YOU MAY ALSO LIKE</h4>
    </div>
    
    <ul class="suggestion-list">
      <% blog.articles.select{ |article| article.data.category == 'Profile' }.shuffle.uniq[0...3].each do |a| %>
      <li class="interviewee-suggestion-item">
        <img src="//images.contentful.com/<%= a.data.contentful_profile_pic %>">
        <a class='other-episodes' href="/episodes/<%= a.data.episode_number %>.html"><%= a.data.interviewee %></a>
      </li>
      <% end %>
    </ul>
